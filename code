#include <iostream>
#include <vector>

using namespace std;

vector<int> vec;

int main()
{
	int n;
	bool impar = false;

	cin >> n;
	vec.resize(n);

	int i;
	for (auto& i : vec)
	{
		cin >> i;

		if (i % 2){
			impar = true;
		}
	}

	if (impar)
	{
		cout << "Unele carti au numar de pagini impar.";
	}
	else
	{
		cout << "Numarul de pagini al tuturor cartilor este par.";
	}
}
